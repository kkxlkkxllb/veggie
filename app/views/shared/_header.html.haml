#header_contain
	#title
		%span
			17up
			= @subtitle
		%span.logo
	- if params[:pid]
		#tag{:pid => "#{params[:pid]}"}
			%span.label.label-success
				%a{:href => leafs_path}
					= Provider.find(params[:pid]).user_name
					&times;
	- if current_member
		#author
			%a(href="/admin" target="blank")
				veggie
				%i.icon-chevron-right